<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lj.iot.biz.db.smart.mapper.IrDataMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lj.iot.biz.db.smart.entity.IrData">
        <id column="id" property="id" />
        <result column="file_id" property="fileId" />
        <result column="data_index" property="dataIndex" />
        <result column="ir_data" property="irData" />
    </resultMap>
    <select id="getDjAirContData" resultType="com.lj.iot.biz.db.smart.entity.IrData">
        select * from ir_data
        where file_id =#{fileIds}
    </select>


</mapper>
